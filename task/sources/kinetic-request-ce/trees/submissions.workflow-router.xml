                <visible>true</visible>
                <parameters>
                    <parameter id="input" label="Input" menu="" required="true" tooltip="">&lt;%= 
# Type of event this tree expects
event_type = 'Submission'
# Name of the attribute
attribute_name = 'Submission Workflow'
# Attributes to check
attributes = @form_attributes

# Returns an array of unique attribute values that match the event action.
#   - strip the action from the resulting value
def action_values(attributes, attribute_name, action)
    pattern = "^#{action}\s-\s(.*)$"
    (attributes.has_key?(attribute_name) ? 
        attributes[attribute_name] : 
        []
    ).select { |value|
        /#{pattern}/.match(value) &amp;&amp; /#{pattern}/.match(value).captures
    }.map { |value| 
        captures = /#{pattern}/.match(value).captures
        captures[0]
    }.uniq
end

# Declare the default return variable
values = []
# Check attributes
if @event['Type'] == event_type
    # Check for processes
    values = action_values(attributes, attribute_name, @event['Action'])
    # If no values were present, default to empty array
    values = [] if values.size == 0
end
# return the values as json
values.to_json
%&gt;</parameter>
